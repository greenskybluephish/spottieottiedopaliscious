{
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 0,
        "quotes": ["warn", "double"],
        "eqeqeq": 1,
        "no-trailing-spaces": 1
    }
  }
